@prefix arena: <http://paul.ti.rw.fau.de/~pi69geby/arena/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix http: <http://www.w3.org/2011/http#> .
@prefix httpm: <http://www.w3.org/2011/http-methods#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

{
    ?workstation  a arena:Workstation;
				    arena:hasSlot ?slot.
				  
    ?slot arena:capacity ?capacity

    ?capacity math:greaterThan "0"^^xsd:integer .
    
}
=>
{
    []  http:mthd httpm:POST ;
        http:requestURI ?workstation ;
        http:fieldName "Content-Type" ;
        http:fieldValue "application/sparql-update" ;
        http:body {

            []  a       sp:InsertData ;
                sp:data    ( <current#ip1> ) .

            <current#ip1>    sp:subject screwingStation ;
                    sp:predicate rdf:type ;
                    sp:object arena:Workstation .

        } .
} .