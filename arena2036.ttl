@base <http://localhost:2000/> .
@prefix arena: <http://paul.ti.rw.fau.de/~pi69geby/arena/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .

<current#agentTransfer1> a arena:Agent ;
	arena:program "transfer.n3" .

<current#agentAssemble1> a arena:Agent ;
	arena:program "assemble.n3" .

<current#agentTransfer2> a arena:Agent ;
	arena:program "transfer.n3" .

<current#agentAssemble2> a arena:Agent ;
	arena:program "assemble.n3" .

<current#iotBoard> a schema:ProductModel ;
	schema:potentialAction <current#screwingStationToRackTransfer>, <current#rackToShippingTransfer> ;
	arena:counter 1 .

<current#motherboard> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <current#iotBoard> ;
	schema:potentialAction <current#solderingStationToRackTransfer>, <current#rackToScrewingStationTransfer> ;
	arena:counter 1 .

<current#processingUnit> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <current#motherboard> ;
	schema:potentialAction <current#deliveryToRackTransfer>, <current#rackToSolderingStationTransfer> ;
	arena:counter 1 .

<current#temperatureSensor> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <current#motherboard> ;
	schema:potentialAction <current#deliveryToRackTransfer>, <current#rackToSolderingStationTransfer> ;
	arena:counter 1 .

<current#case> a schema:ProductModel ;
	schema:isAccessoryOrSparePartFor <current#iotBoard> ;
	schema:potentialAction <current#deliveryToRackTransfer>, <current#rackToScrewingStationTransfer> ;
	arena:counter 1 .

<current#deliveryToRackTransfer> a schema:TransferAction ;
	schema:fromLocation <current#delivery> ;
	schema:toLocation <current#rack> ;
	schema:actionStatus schema:PotentialActionStatus .
	
<current#rackToScrewingStationTransfer> a schema:TransferAction ;
	schema:fromLocation <current#rack> ;
	schema:toLocation <current#screwingStation1>, <current#screwingStation2>, <current#screwingStation3>, <current#screwingStation4> ;
	schema:actionStatus schema:PotentialActionStatus .
	
<current#rackToSolderingStationTransfer> a schema:TransferAction ;
	schema:fromLocation <current#rack> ;
	schema:toLocation <current#solderingStation1>, <current#solderingStation2>, <current#solderingStation3>, <current#solderingStation4> ;
	schema:actionStatus schema:PotentialActionStatus .
	
<current#screwingStationToRackTransfer> a schema:TransferAction ;
	schema:fromLocation <current#screwingStation1>, <current#screwingStation2>, <current#screwingStation3>, <current#screwingStation4> ;
	schema:toLocation <current#rack> ;
	schema:actionStatus schema:PotentialActionStatus .

<current#solderingStationToRackTransfer> a schema:TransferAction ;
	schema:fromLocation <current#solderingStation1>, <current#solderingStation2>, <current#solderingStation3>, <current#solderingStation4> ;
	schema:toLocation <current#rack> ;
	schema:actionStatus schema:PotentialActionStatus .

<current#rackToShippingTransfer> a schema:TransferAction ;
	schema:fromLocation <current#rack> ;
	schema:toLocation <current#shipping> ;
	schema:actionStatus schema:PotentialActionStatus .

<current#iotBoardAssemble> a arena:AssembleAction ;
	schema:actionStatus schema:PotentialActionStatus ;
	schema:location <current#screwingStation1>, <current#screwingStation2>, <current#screwingStation3>, <current#screwingStation4> ;
	schema:object <current#motherboard>, <current#case> ;
	schema:result <current#iotBoard> .

<current#motherboardAssemble> a arena:AssembleAction ;
	schema:actionStatus schema:PotentialActionStatus ;
	schema:location <current#solderingStation1>, <current#solderingStation2>, <current#solderingStation3>, <current#solderingStation4> ;
	schema:object <current#processingUnit>, <current#temperatureSensor> ;
	schema:result <current#motherboard> .

<current#deliverAction> a schema:CreateAction ;
	schema:actionStatus schema:PotentialActionStatus ;
	schema:location <current#delivery> ;
	schema:result <current#temperatureSensor>, <current#processingUnit>, <current#case> ;
	arena:rate 2000 .

<current#shipAction> a schema:ConsumeAction ;
	schema:actionStatus schema:PotentialActionStatus ;
	schema:location <current#shipping> ;
	schema:object <current#iotBoard> ;
	arena:rate 10000 .

<current#fl1> a arena:Forklift ;
