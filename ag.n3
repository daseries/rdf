@prefix arena: <http://paul.ti.rw.fau.de/~pi69geby/arena/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix http: <http://www.w3.org/2011/http#> .
@prefix httpm: <http://www.w3.org/2011/http-methods#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

{
    ?action  a schema:TransferAction .
    
    ( ?action "" ) string:uuid ?uuid .
}
=>
{
    []  http:mthd httpm:POST ;
        http:requestURI ?action ;
        http:fieldName "Content-Type" ;
        http:fieldValue "application/sparql-update" ;
        http:body {

            []  a   sp:InsertData ;
                    sp:data    ( <#ip2> <#ip3> <#ip4> <#ip5> <#ip6> <#ip7> ) .

            <#ip2>    sp:subject ?uuid ;
                    sp:predicate rdf:type ;
                    sp:object schema:TransferAction .

            <#ip3>    sp:subject ?uuid ;
                    sp:predicate schema:actionStatus ;
                    sp:object schema:ActiveActionStatus .

            <#ip4>    sp:subject ?uuid ;
                    sp:predicate schema:fromLocation ;
                    sp:object ?fromLocation .

            <#ip5>    sp:subject ?uuid ;
                    sp:predicate schema:toLocation ;
                    sp:object ?toLocation .

            <#ip6>    sp:subject ?uuid ;
                    sp:predicate schema:instrument ;
                    sp:object ?forklift .

            <#ip7>    sp:subject ?uuid ;
                    sp:predicate schema:object ;
                    sp:object ?product .
        } .
} .